MBC5_ENABLE_RAM         EQU $0000       ; RAM Enable
MBC5_LOW_ROM_SELECT     EQU $2000       ; Low ROM Select
MBC5_HGH_ROM_SELECT     EQU $3000       ; High ROM Select
MBC5_RAM_SELECT         EQU $4000       ; RAM Select

MBC5_DIS_RAM_CODE       equ $0          ; Disable RAM Code to write to MBC5_ENABLE_RAM
MBC5_EN_RAM_CODE        equ $A          ; Enable RAM Code to write to MBC5_ENABLE_RAM
